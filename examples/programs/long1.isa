; Long-running guest 1 - Loop that counts up
mov r0, r0      ; r0 = 0 (counter)
mov r1, r0      ; r1 = 0
add r1, r0, r1  ; loop: r1 = r1 + r0 (accumulate)
add r0, r0, r0  ; r0 = r0 + r0 (increment, but doubles)
add r0, r0, r0  ; r0 = r0 + r0
add r0, r0, r0  ; r0 = r0 + r0
add r0, r0, r0  ; r0 = r0 + r0
mov r2, r1      ; r2 = r1 (save result)
halt
